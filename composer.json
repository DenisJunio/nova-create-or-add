{
    "name": "denisjunio/nova-create-or-add",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "DenisJunio\\NovaCreateOrAdd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenisJunio\\NovaCreateOrAdd\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
