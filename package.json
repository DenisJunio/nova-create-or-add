        "eslint-plugin-lodash": "^7.2.0",
        "eslint-plugin-vue": "^4.4.0",
        "laravel-mix": "^6.0.19",
        "laravel-nova": "^1.12.3",
        "lodash": "^4.17.21",
        "postcss": "^8.2.14",
        "postcss-import": "^14.0.1",
        "prettier": "^1.14.0",
        "resolve-url-loader": "^3.1.3"
    },
    "dependencies": {
        "popper.js": "^1.16.1",
        "vue": "^2.6.14",
        "vue-clickaway": "^2.2.2"
